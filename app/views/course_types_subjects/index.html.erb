<p id="notice"><%= notice %></p>

<h1>Listing Course Types Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Course type</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_types_subjects.each do |course_types_subject| %>
      <tr>
        <td><%= course_types_subject.course_type_id %></td>
        <td><%= course_types_subject.subject_id %></td>
        <td><%= link_to 'Show', course_types_subject %></td>
        <td><%= link_to 'Edit', edit_course_types_subject_path(course_types_subject) %></td>
        <td><%= link_to 'Destroy', course_types_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course types subject', new_course_types_subject_path %>
