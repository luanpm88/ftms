<p id="notice"><%= notice %></p>

<h1>Listing Course Types Discount Programs</h1>

<table>
  <thead>
    <tr>
      <th>Course type</th>
      <th>Discount program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_types_discount_programs.each do |course_types_discount_program| %>
      <tr>
        <td><%= course_types_discount_program.course_type_id %></td>
        <td><%= course_types_discount_program.discount_program_id %></td>
        <td><%= link_to 'Show', course_types_discount_program %></td>
        <td><%= link_to 'Edit', edit_course_types_discount_program_path(course_types_discount_program) %></td>
        <td><%= link_to 'Destroy', course_types_discount_program, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course types discount program', new_course_types_discount_program_path %>
