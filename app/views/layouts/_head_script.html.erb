	function calculateTransferHourFrom() {
		var money = 0
		$('.hour_input_group').each(function() {
			var box = $(this)
			var input_remain_hour = 0
			var input_remain_money = 0
			var input_hour = 0
			
			if (box.find(".input_remain_hour").attr("value") != "") {
				input_remain_hour = parseFloat(box.find(".input_remain_hour").attr("value").replace(/,/g,""))  
			}
			if (box.find(".input_remain_money").attr("value") != "") {
				input_remain_money = parseFloat(box.find(".input_remain_money").attr("value").replace(/,/g,""))  
			}
			if (box.find(".input_hour").val() != "") {
				var vv = parseFloat(box.find(".input_hour").val().replace(/,/g,""))
				if(vv > input_remain_hour) {
					vv = input_remain_hour
					box.find(".input_hour").val(ReplaceNumberWithDots(vv.toFixed(0)))
				}
				input_hour = vv
			} else {
				box.find(".input_hour").val(0)
			}
			
			var rate = input_remain_money/input_remain_hour
			var input_new_hour = input_remain_hour - input_hour
			var input_new_money = input_remain_money - rate*input_hour
			
			box.find(".input_remain_hour").html(ReplaceNumberWithDots(input_new_hour.toFixed(0)))
			box.find(".input_remain_money").html(ReplaceNumberWithDots(input_new_money.toFixed(0)))
			
			money += rate*input_hour
			
		})
		$(".transfer_hour_money_total").val(ReplaceNumberWithDots(money.toFixed(0)))
	}
	
	function calculateTransferPayForm() {
		var total = parseFloat($(".transfer_payment_form .payment-record-total").html().replace(/,/g,""))
		var remain = parseFloat($(".transfer_payment_form .payment-record-remain").attr("rel").replace(/,/g,""))
		var paid = 0
		if($(".transfer_payment_form .payment-record-paid").val() != "") {
			paid = parseFloat($(".transfer_payment_form .payment-record-paid").val().replace(/,/g,""))
		}
		
		$(".transfer_payment_form .payment-record-remain").html(ReplaceNumberWithDots((remain-paid).toFixed(0)))
	}
	
