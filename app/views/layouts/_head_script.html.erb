				tid = $("input[name='transfer[to_contact_id]']").val()
			}
			ajaxTransferToBox($(this).select2("val"), fid, tid)
		});
		if($(".transfer_course_from").length) {
			$(".transfer_course_from .from_course").trigger("change")
		}
		
		updatePhraseListBoxHour()
		$(document).on("change", ".courses_phrase_item", function() {
			updatePhraseListBoxHour()
		});
		
		transferToTypeCheck()
		$(document).on("change", "select[name='transfer[to_type]']", function() {
			transferToTypeCheck()
		});
		
		$(document).on("change", "input[name='transfer[to_contact_id]']", function() {
			$("select[name='transfer[course_id]']").trigger("change")			
		});

		$(document).on("keyup", ".transfer_payment_form .payment-record-paid", function() {
			calculateTransferPayForm()
		});
		$(document).on("change", ".transfer_payment_form .payment-record-paid", function() {
			calculateTransferPayForm()
		});
		
		//Seminar check import
		$(".seminar_group input[type=checkbox]").change(function(){
			var group = $(this).attr("group")
			if($(this).attr("checked") == "checked") {
			  $(".seminar_group_"+group).removeClass("row_selected")
			  $(".seminar_group_"+group+" input[type=checkbox]").attr("checked",false)
			  $(this).attr("checked",true)
			}
		})
		
		$(document).on("keyup", ".transfer_hour_from .input_hour", function() {
			calculateTransferHourFrom()
		});
		$(document).on("change", ".transfer_hour_from .input_hour", function() {
			calculateTransferHourFrom()
		});
		$(document).on("click", ".approve_link", function(e) {
			e.preventDefault()
			approve_ajax($(this))
		});
		if($(".datatable-merge-contacts").length) {
			setInterval('$(".datatable-merge-contacts .row-color-true").each(function() {$(this).parents("tr").addClass("color_dark")})', 500)
		}
		if($("input.upfront_radio").length) {
			checkUpfrontButton($("input.upfront_radio:checked"))
		}
		$(document).on("change", "input.upfront_radio", function(e) {
			checkUpfrontButton($(this))
		});
		$(document).on("change", ".program_paper_select_box select.program_select, .program_paper_select_box input.program_select", function(e) {
			ajaxSubjectSelect($(this))
		});
		$(document).on("click", ".uncheckable *", function(e) {
			e.preventDefault()
		});
		
		$(document).on("click", ".ajax-uncheck-book-upfront", function(e) {
			var id = $(this).attr("rel")
			var box = $(this).parents(".upfront-box")
			// load select bc sellect
			$.ajax({
				url : '<%= upfront_book_select_box_books_contacts_path %>?id='+id,
				success:function(data, textStatus, jqXHR)
				{
					box.find(".select-bc-box").html(data)
					box.find(".select-bc-box").show()
					box.find(".select-bc-box select").select2()
				}
			});
		});
		
		$(document).on("change", "select.bc-upfront-select", function(e) {
			var id = $(this).attr("bc_id")
			var book_id = $(this).select2("val")
			var box = $(this).parents(".upfront-box")
			var table = $(this).parents("table")
			// load select bc sellect
			$.ajax({
				url : '<%= check_upfront_books_contacts_path %>?id='+id+'&book_id='+book_id,
				success:function(data, textStatus, jqXHR)
				{					
					table.dataTable().fnFilter();
				}
			});
		});
		
		$(document).on("click", ".ajax-check-book-upfront", function(e) {
			var id = $(this).attr("bc_id")
			var box = $(this).parents(".upfront-box")
			var table = $(this).parents("table")
			// load select bc sellect
			$.ajax({
				url : '<%= check_upfront_books_contacts_path %>?id='+id+'&book_id=',
				success:function(data, textStatus, jqXHR)
				{					
					table.dataTable().fnFilter();
				}
			});
		});		
		
		$(document).on("click", ".remove_related_contact_link", function(e) {
			e.preventDefault()
			var message = $(this).attr("confirm-message")
			var url = $(this).attr("href")
			if (confirm(message)) {
				//$.ajax({
				//	url : url,
				//	success:function(data, textStatus, jqXHR)
				//	{
				//		location = url; 
				//	}
				//});
				window.location = url; 
			}
		})
		$(document).on("click", ".destroy-old-tag", function(e) {
			e.preventDefault()
			var message = "Sure you want to destroy this tag?"
			var url = $(this).attr("href")
			var id = $(this).attr("rel")
			if (confirm(message)) {
				$.ajax({
					url : url,
					method: 'DELETE',
					success:function(data, textStatus, jqXHR)
					{					
						$(".old-tag-row-"+id).hide()
						window.parent.modernAlert("Old tag was deleted!")
					}
				});
			}
		})
		$(document).on("click", ".more_field", function(e) {
			var box = $(this).parents(".more_field_box")
			box.find(".more_field_item").each(function() {
				if ($(this).hasClass("hidden")) {
                    $(this).removeClass("hidden")
					return false;
                }
			})
		});
		
		$(document).on("click", ".note_edit_button", function(e) {
			var edit_box = $(this).parent().find(".note_log_edit_box")
			if (edit_box.css("display") == "none") {
                edit_box.show()
				$(this).parent().find(".note_content").hide()
				$(this).hide()
            }
		});
		$(document).on("click", ".note_cancel_button", function(e) {
			var edit_box = $(this).parents("td").find(".note_log_edit_box")
			if (edit_box.css("display") != "none") {
                edit_box.hide()
				$(this).parents("td").find(".note_content").show()
				$(this).parents("td").find(".note_edit_button").show()
            }
		});
		$(document).on("click", ".note_save_button", function(e) {
			var box = $(this).parents("td")
			var text = encodeURIComponent(box.find("textarea").val())
			var iid = box.find(".note_log_edit_box").attr("item-id")
			$.ajax({
				url : '<%= change_activities_path %>?id='+iid+'&text='+text,
				success:function(data, textStatus, jqXHR)
				{
					window.parent.modernAlert(data)
					box.parents("table").dataTable().fnFilter();
				}
			});
		});
		
		
		$(document).on("click", "input[name='payment_for']", function(e) {
			if ($(this).val() == "company") {
                $(".company_receivable_box").show()
            } else {
				$(".company_receivable_box").hide()
			}
		});
		
		$(document).on("change", "input.display_order_input", function(e) {
			var iid = $(this).attr("rel")
			var val = $(this).val()
			var table = $(this).parents("table")
			$.ajax({
				url : '<%= update_order_stock_types_path %>?id='+iid+'&value='+val,
				success:function(data, textStatus, jqXHR)
				{
					window.parent.modernAlert(data)
					table.dataTable().fnFilter();
				}
			});
		});
		
		$("input[type=search]").attr("name", "datatable_search_keyword")
		
		// FULL COURSE
		fullCourseCheck()
		$(document).on("change", ".full_course_checkbox", function(e) {
			fullCourseCheck()
		});
		
		// money transfer history
		$('.transfer_money_log_button').on('click', function(){
			var iid = $(this).attr("contact-id");
			window.parent.$.fancybox({
				width: "70%",
				height: "70%",
				autoSize: false,
				href: '<%= transfer_money_history_contacts_path %>?id='+iid,
				type: 'ajax'
			});
		});
		
		// hour transfer history
		$('.transfer_hour_log_button').on('click', function(){
			var iid = $(this).attr("contact-id");
			var pd = $(this).attr("program-id");
			var sd = $(this).attr("subject-id");
			window.parent.$.fancybox({
				width: "70%",
				height: "70%",
				autoSize: false,
				href: '<%= transfer_hour_history_contacts_path %>?id='+iid+"&program_id="+pd+"&subject_id="+sd,
				type: 'ajax'
			});
		});
	})
	

</script>