<p id="notice"><%= notice %></p>

<h1>Listing Courses Phrases</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Phrase</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses_phrases.each do |courses_phrase| %>
      <tr>
        <td><%= courses_phrase.course_id %></td>
        <td><%= courses_phrase.phrase_id %></td>
        <td><%= link_to 'Show', courses_phrase %></td>
        <td><%= link_to 'Edit', edit_courses_phrase_path(courses_phrase) %></td>
        <td><%= link_to 'Destroy', courses_phrase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Courses phrase', new_courses_phrase_path %>
