<p id="notice"><%= notice %></p>

<h1>Listing Contacts Course Types</h1>

<table>
  <thead>
    <tr>
      <th>Contact</th>
      <th>Course type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts_course_types.each do |contacts_course_type| %>
      <tr>
        <td><%= contacts_course_type.contact_id %></td>
        <td><%= contacts_course_type.course_type_id %></td>
        <td><%= link_to 'Show', contacts_course_type %></td>
        <td><%= link_to 'Edit', edit_contacts_course_type_path(contacts_course_type) %></td>
        <td><%= link_to 'Destroy', contacts_course_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contacts course type', new_contacts_course_type_path %>
