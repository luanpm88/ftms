<p id="notice"><%= notice %></p>

<h1>Listing Books Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Course register</th>
      <th>Book</th>
      <th>Contact</th>
      <th>Price</th>
      <th>Discount program</th>
      <th>Discount</th>
      <th>Volumn ids</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books_contacts.each do |books_contact| %>
      <tr>
        <td><%= books_contact.course_register_id %></td>
        <td><%= books_contact.book_id %></td>
        <td><%= books_contact.contact_id %></td>
        <td><%= books_contact.price %></td>
        <td><%= books_contact.discount_program_id %></td>
        <td><%= books_contact.discount %></td>
        <td><%= books_contact.volumn_ids %></td>
        <td><%= link_to 'Show', books_contact %></td>
        <td><%= link_to 'Edit', edit_books_contact_path(books_contact) %></td>
        <td><%= link_to 'Destroy', books_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Books contact', new_books_contact_path %>
